                                <li > <a href="{{path('oferta')}}"><i class="fa fa-list-ol"></i> Listado</a> </li>
                                <li > <a href="{{path('oferta_cliente')}}"><i class="fa fa-plus-circle"></i> Adicionar</a> </li>
                            </ul>
                        </li>
                        <!--END SUBMENU OFERTA-->
                        <!--SUBMENU FICHA-->
